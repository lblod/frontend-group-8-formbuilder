{{page-title "Edit"}}
<AppChrome
  @editorDocument={{this.model.editorDocument}}
  @documentContainer={{this.model.documentContainer}}
  @save={{hash action=(perform this.save) isRunning=this.save.isRunning}}
  @publish={{hash action=this.publish}}
  @returnRoute="list"
  @returnRouteText={{t 'reglementEdit.return'}}
/>
<RdfaEditorContainer
  @rdfaEditorInit={{this.handleRdfaEditorInit}}
  @typeOfWrappingDiv="besluit:BehandelingVanAgendapunt"
  @showToggleRdfaAnnotations={{true}}
  />